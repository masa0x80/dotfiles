font-family =
font-family = Explex Console NF
font-size = 14

keybind = ctrl+s>ctrl+c=new_tab
keybind = ctrl+s>c=new_tab
keybind = ctrl+s>ctrl+p=previous_tab
keybind = ctrl+s>p=previous_tab
keybind = ctrl+s>ctrl+n=next_tab
keybind = ctrl+s>n=next_tab

keybind = ctrl+s>-=new_split:up
keybind = ctrl+s>\=new_split:right
keybind = ctrl+s>ctrl+i=goto_split:next
keybind = ctrl+s>i=goto_split:next
keybind = ctrl+s>ctrl+o=goto_split:previous
keybind = ctrl+s>o=goto_split:previous
keybind = ctrl+s>z=toggle_split_zoom
