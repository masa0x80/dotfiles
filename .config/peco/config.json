{
  "Style": {
    "Basic":          ["on_default", "default"],
    "SavedSelection": ["yellow"],
    "Selected":       ["underline", "green"],
    "Query":          ["bold", "white"],
    "Matched":        ["underline", "bold", "white"]
  },
  "Keymap": {
