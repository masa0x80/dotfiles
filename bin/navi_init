#!/usr/bin/env fish

function navi_init
    test -d .cheats.d && return 1

    mkdir -p .cheats.d
    echo '% Local' >>.cheats.d/_.cheat
    echo >>.cheats.d/_.cheat
    echo '# sample' >>.cheats.d/_.cheat
    echo 'echo <msg>' >>.cheats.d/_.cheat
    echo >>.cheats.d/_.cheat
    echo '$ msg: echo -e "yes\nno"' >>.cheats.d/_.cheat
end
navi_init
